- i = owner_counter + 1
%table.table.table-striped{ id: "owner_#{i}" }
  %thead
    %th Owner ##{ i }
    %th
  %tbody
    %tr
      %td{width: '30%'} Owner Name
      %td.owner-name{width: '35%'}
        %strong #{ owner.name }
    %tr
      %td Owner Address
      %td
        %strong= owner.inline_address
%hr
